<div class="container px-5 px-sm-0">

<%= render "users/user_info", user: @user %>
<%= render "books/new", book: @book %>

<h1>Users</h1>

<% @users.each do |user| %>
  <div>
    <%= image_tag user.get_profile_image(50,50), class: "profile-image" %>
    <h2><%= user.name %></h2>
    <%= link_to "Show", user_path(user) %>
  </div>
<% end %>

</div>




